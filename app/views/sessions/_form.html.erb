<md-dialog aria-label="Admin login form">

  <md-content>
    <form ng-submit="submit()" name="form" novalidate>
      <md-content class="md-padding">
        <div class="form-field">
          <md-text-float ng-model="form.username" label="Username"
            type="text" required></md-text-float>
        </div>

        <div class="form-field">
          <md-text-float ng-model="form.password" label="Password"
            type="password" required></md-text-float>
        </div>

        <div class="form-actions">
          <md-button type="submit" class="md-raised md-primary"
            aria-label="Submit">Submit</md-button>
          <md-button type="button" class="md-raised md-warn" ng-click="cancel()"
            aria-label="Cancel">Cancel</md-button>
        </div>
      </md-content>
    </form>
  </md-content>

</md-dialog>
